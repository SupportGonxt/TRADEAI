{
  "name": "trade-ai-backend",
  "version": "2.2.0",
  "description": "Trade AI - Intelligent Trade Spend Management Platform Backend - South African Edition",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:integration": "jest --testPathPattern=integration --detectOpenHandles --forceExit",
    "test:unit": "jest --testPathPattern=unit --detectOpenHandles --forceExit",
    "test:services": "jest --testPathPattern=services --detectOpenHandles --forceExit",
    "test:e2e": "jest --testPathPattern=e2e --detectOpenHandles --forceExit",
    "seed": "node src/seeds/index.js",
    "seed:simple": "node src/seeds/simple-seed.js",
    "seed:minimal": "node src/seeds/minimal-seed.js",
    "seed:production": "node src/seeds/production-seed.js",
    "seed:clean": "node src/seeds/clean-production-seed.js",
    "seed:mondelez": "node src/seeders/mondelez-sa-seed.js",
    "seed:full": "node scripts/seed-production-full.js",
    "seed:demo": "node scripts/seed-comprehensive-demo.js",
    "check-db": "node src/scripts/check-db.js",
    "migrate": "node src/migrations/index.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "ml:train": "node src/ml/train.js",
    "ml:evaluate": "node src/ml/evaluate.js",
    "build": "npm run lint && npm run test",
    "docker:build": "docker build -t tradeai-backend .",
    "docker:run": "docker run -p 3000:3000 tradeai-backend"
  },
  "keywords": [
    "fmcg",
    "trade-spend",
    "marketing",
    "promotions",
    "analytics"
  ],
  "author": "FMCG Corp",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@sendgrid/mail": "^8.1.6",
    "@sentry/node": "^10.17.0",
    "@sentry/profiling-node": "^10.17.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "agenda": "^5.0.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "csv-parser": "^3.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.6.1",
    "exceljs": "^4.3.0",
    "express": "^4.22.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.11.2",
    "express-slow-down": "^3.0.1",
    "express-validator": "^7.3.1",
    "helmet": "^7.2.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.9.3",
    "joi": "^17.10.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "mathjs": "^14.8.2",
    "ml-regression": "^5.0.0",
    "mongoose": "^7.8.8",
    "mongoose-aggregate-paginate-v2": "^1.0.6",
    "mongoose-autopopulate": "^1.2.1",
    "mongoose-sequence": "^6.0.1",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.13.0",
    "qrcode": "^1.5.3",
    "rate-limit-redis": "^4.2.2",
    "redis": "^4.7.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "simple-statistics": "^7.8.8",
    "socket.io": "^4.6.2",
    "speakeasy": "^2.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.18.3",
    "xlsx": "^0.18.5",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "axios": "^1.12.2",
    "eslint": "^8.48.0",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^29.6.4",
    "mongodb-memory-server": "^11.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "directories": {
    "test": "test"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/server.js",
      "!src/config/**",
      "!src/scripts/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testTimeout": 30000,
    "maxWorkers": 1
  }
}
